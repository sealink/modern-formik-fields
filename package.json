  "prettier": "@sealink/prettier-config",
  "author": "Stefan Cooper <stefan.cooper@sealink.com.au>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sealink/modern-formik-fields/issues"
  },
  "homepage": "https://github.com/sealink/modern-formik-fields#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@sealink/prettier-config": "^1.1.0",
    "@storybook/addon-actions": "^6.2.8",
    "@storybook/addon-essentials": "^6.2.8",
    "@storybook/addon-links": "^6.2.8",
    "@storybook/react": "^6.2.8",
    "@team-griffin/install-self-peers": "^1.1.1",
    "babel-loader": "^8.2.2",
    "cssnano": "^5.0.2",
    "formik": ">= 2",
    "lit-html": "^2.0.0",
    "lodash": ">= 4",
    "postcss": "^8.2.13",
    "postcss-cssnext": "^3.1.0",
    "prettier": "^2.2.1",
    "rollup": "^2.46.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-postcss": "^4.0.0",
    "storybook": "^6.2.8"
  },
  "peerDependencies": {
    "formik": ">= 2",
    "lodash": ">= 4",
    "prop-types": ">= 15",
    "react": ">= 16",
    "react-dates": ">= 21",
    "react-dom": ">= 16",
    "react-select": ">= 3"
